<vector

    xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:width="24dp"

    android:height="24dp"

    android:viewportWidth="24.0"

    android:viewportHeight="24.0"

    app:vc_viewportWidth="24.0"

    app:vc_viewportHeight="24.0"

    >

    <group

        android:name="@string/group_button_rotate_right"

        android:pivotX="12"

        android:pivotY="12"

        >

        <path

            android:fillColor="#FF000000"

            android:pathData="@string/path_data_rotate_right"

            app:vc_fillColor="#FF000000"

            app:vc_pathData="@string/path_data_rotate_right"

            />

    </group>

</vector>
